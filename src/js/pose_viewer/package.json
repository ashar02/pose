    "generate": "stencil generate",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@stencil/core": "4.26.0",
    "dt-pose-format": "1.6.2",
    "three": "0.173.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "22.13.4",
    "@types/three": "0.173.0",
    "rollup-plugin-node-polyfills": "0.2.1"
  }
}
